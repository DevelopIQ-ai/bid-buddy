# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key-here
# Get this from: https://supabase.com/dashboard/project/YOUR_PROJECT_ID/settings/api
# Look for "JWT Secret" under Project API keys
SUPABASE_JWT_SECRET=your-supabase-jwt-secret-here

# Primary User Configuration (for single-tenant mode)
# This is the email address of the account that owns the Google Drive and projects
# This should match the email used to sign in to the dashboard
PRIMARY_USER_EMAIL=your-email@example.com

# Google OAuth Credentials (same as configured in Supabase Dashboard)
# These are needed for server-side token refresh when processing emails
# Get these from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret

# External API Keys
AGENTMAIL_API_KEY=your-agentmail-api-key-here
RESEND_API_KEY=your-resend-api-key-here

# Email Configuration
# Email address to forward important emails to (admin/estimator)
FORWARD_EMAIL_ADDRESS=admin@example.com

# AI/LLM Services
# Dedalus Labs API for LLM inference (used in email analysis)
DEDALUS_API_KEY=your-dedalus-api-key-here

# Reducto API for PDF/document extraction (used in bid proposal processing)
REDUCTO_API_KEY=your-reducto-api-key-here

# BuildingConnected/Autodesk Integration
# Password for Autodesk account (used for BuildingConnected proposal downloads)
AUTODESK_PASSWORD=your-autodesk-password-here

# Sentry Error Tracking (Optional)
# Get your DSN from: https://sentry.io/settings/YOUR_ORG/projects/YOUR_PROJECT/keys/
SENTRY_DSN=https://YOUR_KEY@YOUR_ORG.ingest.sentry.io/YOUR_PROJECT_ID
# Optional Sentry Configuration
SENTRY_ENVIRONMENT=development  # or production, staging, etc.
SENTRY_RELEASE_VERSION=1.0.0
SENTRY_TRACES_SAMPLE_RATE=1.0  # 1.0 = 100% of transactions, 0.1 = 10%
SENTRY_PROFILES_SAMPLE_RATE=1.0  # 1.0 = 100% profiling, 0.1 = 10%
SENTRY_SEND_PII=true  # Include user data like IP addresses
