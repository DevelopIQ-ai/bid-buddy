# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key-here
# Get this from: https://supabase.com/dashboard/project/YOUR_PROJECT_ID/settings/api
# Look for "JWT Secret" under Project API keys
SUPABASE_JWT_SECRET=your-supabase-jwt-secret-here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# External API Keys
AGENTMAIL_API_KEY=your-agentmail-api-key-here
RESEND_API_KEY=your-resend-api-key-here

# Email Configuration
# Email address to forward important emails to (admin/estimator)
FORWARD_EMAIL_ADDRESS=admin@example.com

# AI/LLM Services
# Dedalus Labs API for LLM inference (used in email analysis)
DEDALUS_API_KEY=your-dedalus-api-key-here

# Reducto API for PDF/document extraction (used in bid proposal processing)
REDUCTO_API_KEY=your-reducto-api-key-here
